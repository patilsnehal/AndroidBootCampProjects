import com.codepath.apps.basictwitter.models.Tweet;


public class TweetArrayAdapter extends ArrayAdapter<Tweet> {
	public TweetArrayAdapter(Context context, List<Tweet> tweets){
		super(context, 0 , tweets);
	}
}
